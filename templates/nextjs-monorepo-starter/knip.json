{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "entry": [],
      "project": ["*.{js,ts}"]
    },
    "apps/web": {
      "entry": ["app/**/*.{ts,tsx}", "components/**/*.{ts,tsx}"],
      "project": ["**/*.{ts,tsx}"],
      "ignore": [".next/**"]
    },
    "packages/ui": {
      "entry": ["src/**/*.{ts,tsx}"],
      "project": ["src/**/*.{ts,tsx}"]
    },
    "packages/typescript-config": {
      "entry": [],
      "project": ["*.json"]
    }
  },
  "ignore": ["**/node_modules/**", "**/.next/**", "**/dist/**"],
  "ignoreDependencies": ["@types/*"]
}
