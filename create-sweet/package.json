{
  "name": "create-sweet-starter",
  "version": "0.0.1",
  "description": "CLI tool to scaffold starter templates with Next.js, React, and Tauri",
  "type": "module",
  "bin": {
    "create-sweet": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --target node20 --clean",
    "dev": "tsup src/index.ts --format esm --target node20 --watch",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "create",
    "starter",
    "template",
    "nextjs",
    "react",
    "tauri",
    "monorepo",
    "scaffold",
    "cli"
  ],
  "author": "zhangzimin",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "execa": "^9.6.1",
    "giget": "^3.1.2",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.0"
  }
}
